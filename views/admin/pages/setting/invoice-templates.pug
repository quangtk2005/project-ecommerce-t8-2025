block main
  div(class="settings-content")
    div(class="page-header settings-pg-header")
      div(class="add-item d-flex")
        div(class="page-title")
          h4 Settings
          h6 Manage your settings on portal
      ul(class="table-top-head")
        li
          a(data-bs-toggle="tooltip", data-bs-placement="top", title="Refresh")
            i(class="ti ti-refresh")
        li
          a#collapse-header(data-bs-toggle="tooltip", data-bs-placement="top", title="Collapse")
            i(class="ti ti-chevron-up")
    div(class="row")
      div(class="col-xl-12")
        div(class="settings-wrapper d-flex")
          include ./partials/sidebar2.pug
          div(class="card flex-fill mb-0")
            form(action="invoice-settings.html")
              div(class="card-header")
                h4 Invoice Templates
              div(class="card-body pb-0")
                div(class="d-flex flex-wrap justify-content-between align-items-center mb-3")
                  ul#pills-tab(class="nav nav-pills low-stock-tab d-flex me-2 mb-0", role="tablist")
                    li(class="nav-item", role="presentation")
                      button#pills-home-tab(class="nav-link active", data-bs-toggle="pill", data-bs-target="#pills-home", type="button", role="tab", aria-controls="pills-home", aria-selected="true") Invoices
                    li(class="nav-item", role="presentation")
                      button#pills-profile-tab(class="nav-link", data-bs-toggle="pill", data-bs-target="#pills-profile", type="button", role="tab", aria-controls="pills-profile", aria-selected="false") Purchases
                    li(class="nav-item", role="presentation")
                      button#pills-profile-tab2(class="nav-link", data-bs-toggle="pill", data-bs-target="#pills-profile2", type="button", role="tab", aria-controls="pills-profile2", aria-selected="false") Receipts
                div#pills-tabContent(class="tab-content")
                  div#pills-home(class="tab-pane fade show active", role="tabpanel", aria-labelledby="pills-home-tab")
                    div(class="row gx-3")
                      div(class="col-xl-3 col-lg-4 col-mg-6")
                        div(class="card bg-light invoice-card")
