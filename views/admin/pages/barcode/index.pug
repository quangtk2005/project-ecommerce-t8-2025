extends ../../layout/default.pug

block css
  //- Datetimepicker CSS
  link(rel="stylesheet", href="/admin/assets/css/bootstrap-datetimepicker.min.css")
  //- animation CSS
  link(rel="stylesheet", href="/admin/assets/css/animate.css")
  //- Quill CSS
  link(rel="stylesheet", href="/admin/assets/plugins/quill/quill.snow.css")
  //- Datatable CSS
  link(rel="stylesheet", href="/admin/assets/css/dataTables.bootstrap5.min.css")
  //- Owl Carousel
  link(rel="stylesheet", href="/admin/assets/css/owl.carousel.min.css")

block js
  //- Owl Carousel
  script(src="/admin/assets/js/owl.carousel.min.js")
  //- Datatable JS
  script(src="/admin/assets/js/jquery.dataTables.min.js")
  script(src="/admin/assets/js/dataTables.bootstrap5.min.js")
  //- Quill JS
  script(src="/admin/assets/plugins/quill/quill.min.js")
  //- Chart JS
  script(src="/admin/assets/plugins/apexchart/apexcharts.min.js")
  script(src="/admin/assets/plugins/apexchart/chart-data.js")
  //- Sticky-sidebar
  script(src="/admin/assets/plugins/theia-sticky-sidebar/ResizeSensor.js")
  script(src="/admin/assets/plugins/theia-sticky-sidebar/theia-sticky-sidebar.js")

block main
  div(class="page-header")
    div(class="add-item d-flex")
      div(class="page-title")
        h4(class="fw-bold") Print Barcode
        h6 Manage your barcodes
    div(class="d-flex align-items-center")
      ul(class="table-top-head")
        li
          a(data-bs-toggle="tooltip", data-bs-placement="top", title="Refresh")
            i(class="ti ti-refresh")
        li
          a#collapse-header(data-bs-toggle="tooltip", data-bs-placement="top", title="Collapse")
            i(class="ti ti-chevron-up")
  div(class="barcode-content-list")
    form
      div(class="row")
        div(class="col-lg-6 col-12")
          div(class="row seacrh-barcode-item mb-1")
            div(class="col-sm-6 mb-3 seacrh-barcode-item-one")
              label(class="form-label") Warehouse
                span(class="text-danger ms-1") *
              select(class="select")
                option Select
                option Lavish Warehouse
                option Quaint Warehouse
                option Traditional Warehouse
                option Cool Warehouse
                option Overflow Warehouse
                option Nova Storage Hub
                option Retail Supply Hub
                option EdgeWare Solutions
            div(class="col-sm-6 mb-3 seacrh-barcode-item-one")
              label(class="form-label") Store
                span(class="text-danger ms-1") *
              select(class="select")
                option Select
                option Electro Mart
                option Quantum Gadgets
                option Prime Bazaar
                option Gadget World
                option Volt Vault
      div(class="row")
        div(class="col-lg-6")
          div(class="mb-3 search-form seacrh-barcode-item")
            div(class="search-form")
              label(class="form-label") Product
                span(class="text-danger ms-1") *
              div(class="position-relative")
                input(type="text", class="form-control", placeholder="Search Product by Code")
                i(data-feather="search", class="feather-search")
              div(class="dropdown-menu search-dropdown w-100 h-auto rounded-1 mt-2", aria-labelledby="dropdownsearchClickable")
                ul
                  li(class="fs-14 text-gray-9 mb-2") Amazon Echo Dot
                  li(class="fs-14 text-gray-9 mb-2") Armani Belt
                  li(class="fs-14 text-gray-9 mb-2") Apple  Watch
                  li(class="fs-14 text-gray-9") Apple Iphone 14 Pro
    div(class="col-lg-12")
      div(class="p-3 bg-light rounded border mb-3")
        div(class="table-responsive rounded border")
          table(class="table")
            thead
              tr
                th Product
                th SKU
                th Code
                th Qty
                th(class="text-center no-sort bg-secondary-transparent")
            tbody
              tr
                td
                  div(class="d-flex align-items-center")
                    a(href="javascript:void(0);", class="avatar avatar-md me-2")
                      img(src="/admin/assets/img/products/stock-img-02.png", alt="product")
                    a(href="javascript:void(0);") Nike Jordan
                td PT002
                td HG3FK
                td
                  div(class="product-quantity border-secondary-transparent")
                    span(class="quantity-btn")
                      i(data-feather="minus-circle", class="feather-search")
                    input(type="text", class="quntity-input", value="4")
                    span(class="quantity-btn") +
                      i(data-feather="plus-circle", class="plus-circle")
                td(class="action-table-data")
                  div(class="edit-delete-action")
                    a(data-bs-toggle="modal", data-bs-target="#delete-modal", class="barcode-delete-icon", href="javascript:void(0);")
                      i(data-feather="trash-2", class="feather-trash-2")
              tr
                td
                  div(class="d-flex align-items-center")
                    a(href="javascript:void(0);", class="avatar avatar-md me-2")
                      img(src="/admin/assets/img/products/stock-img-03.png", alt="product")
                    a(href="javascript:void(0);") Apple Series 5 Watch
                td PT003
                td TEUIU7
                td
                  div(class="product-quantity border-secondary-transparent")
                    span(class="quantity-btn")
                      i(data-feather="minus-circle", class="feather-search")
                    input(type="text", class="quntity-input", value="4")
                    span(class="quantity-btn") +
                      i(data-feather="plus-circle", class="plus-circle")
                td(class="action-table-data")
                  div(class="edit-delete-action")
                    a(data-bs-toggle="modal", data-bs-target="#delete-modal", class="barcode-delete-icon", href="javascript:void(0);")
                      i(data-feather="trash-2", class="feather-trash-2")
    div(class="paper-search-size")
      div(class="row align-items-center")
        div(class="col-lg-6")
          form(class="mb-0")
            label(class="form-label") Paper Size
              span(class="text-danger ms-1") *
            select(class="select")
              option Select
              option A3
              option A4
              option A5
              option A6
        div(class="col-lg-6 pt-3")
          div(class="row")
            div(class="col-sm-4")
              div(class="search-toggle-list")
                p Show Store Name
                div(class="m-0")
                  div(class="status-toggle modal-status d-flex justify-content-between align-items-center")
                    input#user7(type="checkbox", class="check", checked)
                    label(for="user7", class="checktoggle mb-0")
            div(class="col-sm-4")
              div(class="search-toggle-list")
                p Show Product Name
                div(class="m-0")
                  div(class="status-toggle modal-status d-flex justify-content-between align-items-center")
                    input#user8(type="checkbox", class="check", checked)
                    label(for="user8", class="checktoggle mb-0")
            div(class="col-sm-4")
              div(class="search-toggle-list")
                p Show Price
                div(class="m-0")
                  div(class="status-toggle modal-status d-flex justify-content-between align-items-center")
                    input#user9(type="checkbox", class="check", checked)
                    label(for="user9", class="checktoggle mb-0")
    div(class="search-barcode-button")
      a(href="javascript:void(0);", class="btn btn-submit btn-primary me-2 mt-0", data-bs-toggle="modal", data-bs-target="#prints-barcode")
        span
          i(class="fas fa-eye me-1")
        | Generate Barcode
      a(href="javascript:void(0);", class="btn btn-cancel btn-secondary fs-13 me-2")
        span
          i(class="fas fa-power-off me-1")
        | Reset Barcode
      a(href="javascript:void(0);", class="btn btn-cancel btn-danger close-btn")
        span
          i(class="fas fa-print me-1")
        | Print Barcode
