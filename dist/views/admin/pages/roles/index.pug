                    a(href=`${prefixAdmin}/role/permission`, class="me-2 d-flex align-items-center p-2 border rounded")
                      i(class="ti ti-shield")
                    a(href="roles-permissions.html#", class="me-2 d-flex align-items-center p-2 border rounded", data-bs-toggle="modal", data-bs-target="#edit-role")
                      i(class="ti ti-edit")
                    a(href="roles-permissions.html#", data-bs-toggle="modal", data-bs-target="#delete_modal", class=" d-flex align-items-center p-2 border rounded")
                      i(class="ti ti-trash")
