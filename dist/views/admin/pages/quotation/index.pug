      li
        a#collapse-header(data-bs-toggle="tooltip", data-bs-placement="top", title="Collapse")
          i(class="ti ti-chevron-up")
    div(class="page-btn")
      a(href="quotation-list.html#", class="btn btn-primary", data-bs-toggle="modal", data-bs-target="#add-quotation")
        i(class="ti ti-circle-plus me-1")
        | Add Quotation
  // /product list
