extends ../../layout/default.pug

block css
  // animation CSS
  link(rel="stylesheet", href="assets/css/animate.css")
  // Datetimepicker CSS
  link(rel="stylesheet", href="assets/css/bootstrap-datetimepicker.min.css")
  // Datatable CSS
  link(rel="stylesheet", href="assets/css/dataTables.bootstrap5.min.css")
  // Bootstrap Tagsinput CSS
  link(rel="stylesheet", href="assets/plugins/bootstrap-tagsinput/bootstrap-tagsinput.css")
  // Tabler Icon CSS
  link(rel="stylesheet", href="assets/plugins/tabler-icons/tabler-icons.min.css")
  // Color Picker Css
  link(rel="stylesheet", href="assets/plugins/@simonwep/pickr/themes/nano.min.css")

block js
  // Datatable JS
  script(src="assets/js/jquery.dataTables.min.js")
  script(src="assets/js/dataTables.bootstrap5.min.js")
  // Datetimepicker JS
  script(src="assets/js/moment.min.js")
  script(src="assets/js/bootstrap-datetimepicker.min.js")
  // Bootstrap Tagsinput JS
  script(src="assets/plugins/bootstrap-tagsinput/bootstrap-tagsinput.js")
  // Color Picker JS
  script(src="assets/plugins/@simonwep/pickr/pickr.es5.min.js")
  // Custom JS
  script(src="assets/js/theme-colorpicker.js")


block main
  div(class="page-header")
    div(class="add-item d-flex")
      div(class="page-title")
        h4(class="fw-bold") Variant Attributes
        h6 Manage your variant attributes
    ul(class="table-top-head")
      li
        a(data-bs-toggle="tooltip", data-bs-placement="top", title="Pdf")
          img(src="assets/img/icons/pdf.svg", alt="img")
      li
        a(data-bs-toggle="tooltip", data-bs-placement="top", title="Excel")
          img(src="assets/img/icons/excel.svg", alt="img")
      li
        a(data-bs-toggle="tooltip", data-bs-placement="top", title="Refresh")
          i(class="ti ti-refresh")
      li
        a#collapse-header(data-bs-toggle="tooltip", data-bs-placement="top", title="Collapse")
          i(class="ti ti-chevron-up")
    div(class="page-btn")
      a(href="varriant-attributes.html#", class="btn btn-primary", data-bs-toggle="modal", data-bs-target="#add-variant")
        i(class="ti ti-circle-plus me-1")
        | Add Variant
  // /product list
  div(class="card")
    div(class="card-header d-flex align-items-center justify-content-between flex-wrap row-gap-3")
      div(class="search-set")
        div(class="search-input")
          span(class="btn-searchset")
            i(class="ti ti-search fs-14 feather-search")
      div(class="table-dropdown my-xl-auto right-content")
        div(class="dropdown")
          a(href="javascript:void(0);", class="dropdown-toggle btn btn-white btn-md d-inline-flex align-items-center", data-bs-toggle="dropdown")
            | Status
          ul(class="dropdown-menu  dropdown-menu-end p-3")
            li
              a(href="javascript:void(0);", class="dropdown-item rounded-1") Active
            li
              a(href="javascript:void(0);", class="dropdown-item rounded-1") Inactive
    div(class="card-body p-0")
      div(class="table-responsive")
        table(class="table datatable")
          thead(class="thead-light")
            tr
              th(class="no-sort")
                label(class="checkboxs")
                  input#select-all(type="checkbox")
                  span(class="checkmarks")
              th Variant
              th Values
              th Created Date
              th Status
              th(class="no-sort")
          tbody
            tr
              td
                label(class="checkboxs")
                  input(type="checkbox")
                  span(class="checkmarks")
              td(class="text-gray-9") Size
              td XS, S, M, L, XL
              td 24 Dec 2024
              td
                span(class="badge table-badge bg-success fw-medium fs-10") Active
              td(class="action-table-data")
                div(class="edit-delete-action")
                  a(class="me-2 p-2", href="varriant-attributes.html#", data-bs-toggle="modal", data-bs-target="#edit-variant")
                    i(data-feather="edit", class="feather-edit")
                  a(data-bs-toggle="modal", data-bs-target="#delete-modal", class="p-2", href="javascript:void(0);")
                    i(data-feather="trash-2", class="feather-trash-2")
